<html>

<head>

    <%- include('../views/partials/links'); %>  
   
</head>

<body>
    <%- include('../views/partials/userHeader'); %>

    <div class="container" style="margin-top: 70px;">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Username</th>
              <th>Email ID</th>
            </tr>
          </thead>
         
          <tbody>
            <% userdata.forEach(user=> { %>
            <tr>
              <td> <%= user.username %> </td>
              <td> <%= user.emailID %> </td>
            </tr>
            <% }) %>
          </tbody>

        </table>
      </div>
      

    <%- include('../views/partials/footer'); %>
</body>

</html>